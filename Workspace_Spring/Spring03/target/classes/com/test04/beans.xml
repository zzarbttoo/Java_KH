<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id=  "lee" class = "com.test04.Emp">
		<property name="name" value = "이순신"></property>
		<property name="salary" value = "3000000"></property>
	</bean>
	
	<bean id = "hong" class = "com.test04.Emp">
		<property name="name" value ="홍길동"></property>
		<property name="salary" value = "250000"></property>
	</bean>
	
	
	<!-- 객체를 주입 -->
	<!-- 첫번째 타입을 참조 해올 것이다 -->
	<!-- Emp 값은 주솟값이다 , 이때는 ref 쓰는 것이다-->
	<bean id = "leess" class = "com.test04.Developer">
		<constructor-arg index = "0" ref = "lee"></constructor-arg>
		<constructor-arg index = "1" value="개발"></constructor-arg>
	</bean>
	
	<bean id = "honggd" class = "com.test04.Engineer">
		<constructor-arg name = "emp" ref = "hong"></constructor-arg>
		<constructor-arg name = "dept" value = "기술"></constructor-arg>
	</bean>

</beans>
