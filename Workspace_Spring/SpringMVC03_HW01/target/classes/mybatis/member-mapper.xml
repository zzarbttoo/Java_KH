<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace= "com.mymember">

<select id="login" parameterType ="myMemberDto" resultType ="myMemberDto">
		SELECT MEMBERNO, MEMBERID, MEMBERPW, MEMBERNAME
		FROM MYMEMBER
		WHERE MEMBERID = #{memberid}
		AND MEMBERPW = #{memberpw}
	</select>
	
	<insert id = "register" parameterType = "myMemberDto">
		INSERT INTO MYMEMBER 
		VALUES(MEMBERSEQ.NEXTVAL, #{memberid},#{memberpw},#{membername})
	</insert>

</mapper>
