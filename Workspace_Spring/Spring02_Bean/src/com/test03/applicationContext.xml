<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- setter로 injection을 진행한다 -->
	<bean id = "lee" class = "com.test03.Address">
		<property name = "name" value = "이순신"/>
		<property name = "addr" value = "서울시 강남구" />
		<property name = "phone" value = "010-1111-1111" />
	</bean>
	
	<!-- 
		property는 setter을 말한다
		propertyname = set"name"을 말한다 
		
		Address lee = new Address();
		lee.setName("이순신");
		lee.setAddr("서울시 강남구");
		lee.setPhone("010-1111-1111");
	 -->
	
	<bean id = "hong" class = "com.test03.Address">
		<property name = "name" value = "홍길동"/>
		<property name = "addr" value = "경기도 수원시"/>
		<property name = "phone" value = "010-3222-2222"/>
	</bean>
	

</beans>
