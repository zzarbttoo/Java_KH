package com.test03;

public class StarPrn {

	public static void main(String[] args) {

		StarPrn star = new StarPrn();

		star.one();
		star.two();
		star.three();
		star.four();
		star.five();

	}

	public void one() {
		/*
		 * * ** *** **** *****
		 */

		for (int i = 0; i < 6; i++) {

			for (int j = 0; j < i; j++) {
				System.out.print("*");
			}
			System.out.println();

		}

	}

	public void two() {

		/*
		 * ***** **** *** ** *
		 */
	}

	public void three() {

		// *
		// **
		// ***
		// ****
		// *****

	}

	public void four() {
		// *****
		// ****
		// ***
		// **
		// *
	}

	public void five() {
		// *
		// ***
		// *****
		// *******
		// *********

	}

}
