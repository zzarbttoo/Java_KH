package com.test01;

public class MTest01 {

	
	public static void main(String[] args) {
		
		
		prn01();
		
	}
	

	public static void prn01() {
		
		int i = 10;
		int j = 20;
		
		//if 
		if (i > j) {
			System.out.println("i는 j보다 작습니다.");
			
		}else{
			System.out.println("i가 j보다 크지 않습니다");
		}
	}
	
}
