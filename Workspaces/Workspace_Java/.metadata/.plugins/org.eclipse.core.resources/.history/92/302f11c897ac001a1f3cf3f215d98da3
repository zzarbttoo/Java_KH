package com.test01;

public class Gugudan {
	
	
	
	public static void main(String[] args) {
	
		for (int i=0;i<10;i++) {
		
			System.out.println("<"+ i+"단>");
			for (int j=0; j<10;j++) {
				System.out.println(i + "*" + j + "=" +i*j);
			}
		}
	/*
	 * <2단>
	 * 2*1 = 2
	 * 2*2 = 4
	 * 
	 * 
	 * <9단>
	 * 9*1 = 9
	 * 
	 * 
	 * 
	 * 
	 */


	}
	}
