package com.test02;

public class Season {
	
//	static String seoson=""; //전역 변수로 해도 된다
	
	
		public String prn(int i) {
			
			String seoson = null; //일단 초기화 시켜줘야함
			
			switch(i) {
			
			case 1 :
			case 2:
			case 12: seoson= "겨울";
			break;
			
			case 3: case 4: case 5: 
			seoson="봄";
			break;
			
			case 6:
			case 7:
			case 8:  seoson="여름";
			break;
				
				
			case 9:
			case 10:
			case 11:  seoson="가을";
			break;
			}
			
			return seoson;
			//처음 시작할 때 return null 쓰면 빨간줄 사라지니까 임시로 사용해보자
			
		}
		
		
		
	
	
}
