package com.test01;

public class MTest01 {

	
	public static void main(String[] args) {
		prn01();
		prn02();
	}
	
	
	public static void prn01() {
		int i = 100;
		 
		while( i < 10) {
			System.out.println(i);
			i++;
		}
		System.out.println("i : " + i);
	}
	
	public static void prn02() {
		
		
		int i = 100;
		
		do {
			System.out.println(i);
			i++;
		} while(i<10);
		
		System.out.println("i : " + i);
	}
}
