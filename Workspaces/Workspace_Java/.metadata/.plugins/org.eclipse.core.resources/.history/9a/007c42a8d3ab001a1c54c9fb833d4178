package com.test02;

public class Season {
	String seoson;
	
		public String prn(int i) {
			switch(i) {
			
			case 1:
			case 2:
			case 12: seoson= "겨울";
			break;
			
			case 3:
			case 4:
			case 5: seoson="봄";
			break;
			
			case 6:
			case 7:
			case 8:  seoson="여름";
			break;
				
				
			case 9:
			case 10:
			case 11:  seoson="가을";
			break;
			}
			
			return seoson;
			
		}
		
		
		
	
	
}
